{
  "name": "kotomi-ui",
  "version": "0.0.34",
  "description": "基于Ant Design做的组件扩展，开发的一套组件库",
  "scripts": {
    "start": "start-storybook -p 3000",
    "build": "tsc && cpx \"components/**/*.less\" ./build/lib",
    "build:typedoc": "typedoc --out build/docs/typedoc",
    "build:storybook": "build-storybook -o docs/storybook/",
    "lint": "eslint components/* --ext .js,.jsx,.ts,.tsx",
    "test": "jest"
  },
  "main": "build/lib/index.js",
  "files": [
    "build/lib"
  ],
  "repository": "https://github.com/Kotomi-Team/kotomi-ui",
  "homepage": "https://github.com/Kotomi-Team/kotomi-ui",
  "keywords": ["React","Ant Design","Table"],
  "author": "zhangj",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/preset-ant-design": "^0.0.1",
    "@storybook/preset-typescript": "^1.1.0",
    "@storybook/react": "^5.2.8",
    "@types/jest": "^24.0.23",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "babel-loader": "^8.0.6",
    "cpx": "^1.5.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^4.0.0",
    "jest": "^24.9.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "typedoc": "^0.15.3",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "antd": "^3.26.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
